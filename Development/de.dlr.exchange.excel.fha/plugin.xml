<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="de.dlr.exchange.excel.fha.sync"
            name="FHA Synchronization">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.dlr.exchange.excel.fha.SyncExecutableExtensionFactory:de.dlr.exchange.base.xtend.ui.GenerationHandler"
            commandId="de.dlr.exchange.excel.fha.sync">
      </handler>
      
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated fha -->
      <package
            uri="www.dlr.de/ft/ssy/fha"
            class="FHA.FHAPackage"
            genModel="model/fha.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:edi_exp_sync">
         <command
               commandId="de.dlr.exchange.excel.fha.sync"
               icon="icons/Hazard.gif"
               label="FHA"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate>
                  <instanceof
                        value="de.dlr.premise.functions.UseCase">
                  </instanceof>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:nav_exp_sync">
         <command
               commandId="de.dlr.exchange.excel.fha.sync"
               icon="icons/Hazard.gif"
               label="FHA"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <or>
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <test
                              property="org.eclipse.core.resources.name"
                              value="*.usecase">
                        </test>
                     </adapt>
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <test
                              property="org.eclipse.core.resources.name"
                              value="*.function">
                        </test>
                     </adapt>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:de.dlr.premise.functions.presentation.UseCaseEditorID">
         <menu
               icon="icons/notebook--sync.png"
               id="edi_exp_sync"
               label="Synchronize">
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:premise.rcp.navigator">
         <menu
               icon="icons/notebook--sync.png"
               id="nav_exp_sync"
               label="Synchronize">
         </menu>
      </menuContribution>
   </extension>

</plugin>
