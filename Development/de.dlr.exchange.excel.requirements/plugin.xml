<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.dlr.exchange.excel.requirements.RequirementsTableGenerationHandler"
            id="de.dlr.exchange.excel.requirements.RequirementsTableGenerationCommand"
            name="Requirements Table">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:nav_exp_arch">
         <command
               commandId="de.dlr.exchange.excel.requirements.RequirementsTableGenerationCommand"
               hoverIcon="icons/reqs.png"
               icon="icons/reqs.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <or>
                     <reference
                           definitionId="de.dlr.exchange.base.expressions.IsFunctionFile">
                     </reference>
                     <reference
                           definitionId="de.dlr.exchange.base.expressions.IsSystemFile">
                     </reference>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:edi_exp_arch">
         <command
               commandId="de.dlr.exchange.excel.requirements.RequirementsTableGenerationCommand"
               hoverIcon="icons/reqs.png"
               icon="icons/reqs.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <instanceof
                        value="de.dlr.premise.element.AElement">
                  </instanceof>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:premise.rcp.navigator">
         <menu
               icon="icons/notebook--arrow.png"
               id="nav_exp_arch"
               label="Export Architecture">
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.dlr.premise.system.presentation.SystemEditorID">
         <menu
               icon="icons/notebook--arrow.png"
               id="edi_exp_arch"
               label="Export Architecture">
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.dlr.premise.functions.presentation.UseCaseEditorID">
         <menu
               icon="icons/notebook--arrow.png"
               id="edi_exp_arch"
               label="Export Architecture">
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.dlr.premise.component.presentation.ComponentEditorID">
         <menu
               icon="icons/notebook--arrow.png"
               id="edi_exp_arch"
               label="Export Architecture">
         </menu>
      </menuContribution>
   </extension>

</plugin>
